{
  "apps": [
    {
      "name": "nest",
      "script": "npm",
      "args": "run start:nest:dev",
      "instances": 3,
      "exec_mode": "cluster",
      "interpreter": "node@20.11.0",
      "env": {
        "NODE_ENV": "development"
      }
    },
    {
      "name": "node",
      "script": "npm",
      "args": "run start:node:dev",
      "instances": 1,
      "exec_mode": "fork",
      "interpreter": "node@20.11.0",
      "env": {
        "NODE_ENV": "development"
      }
    }
  ]
}
